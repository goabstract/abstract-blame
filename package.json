{
  "name": "abstract-blame",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "abstract-blame",
    "manifest": "src/manifest.json",
    "main": "abstract-blame.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
