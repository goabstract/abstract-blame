  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2",
    "@babel/polyfill": "7.0.0"
  },
  "dependencies": {
    "abstract-sdk": "^0.2.6",
    "@skpm/fs": "0.2.0",
    "@skpm/child_process": "0.2.4",
    "@skpm/buffer": "0.1.0",
    "@skpm/events": "0.1.1"
  },
  "author": "Kevin Smith <kevin@kevinsmith.cc>"
}
